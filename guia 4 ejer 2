# ejercicio2
# Contar aprobados y desaprobados en un vector de notas

n = int(input("Ingrese la cantidad de notas: "))
notas = []

aprobados = 0
desaprobados = 0

for i in range(n):
    nota = float(input(f"Ingrese la nota {i+1}: "))
    notas.append(nota)
    if nota >= 6:
        aprobados += 1
    else:
        desaprobados += 1

print(f"\nNotas: {notas}")
print(f"Aprobados: {aprobados}")
print(f"Desaprobados: {desaprobados}")
